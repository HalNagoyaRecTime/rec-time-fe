{
  "name": "my-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node sync-version.js && react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "vite:dev": "vite",
    "vite:build": "node sync-version.js && vite build",
    "build:staging": "cross-env VITE_API_BASE_URL=https://stg.example.com node sync-version.js && react-router build",
    "add-event": "node scripts/addFutureEvent.js",
    "format": "prettier --write \"app/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"app/**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.7.1",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@react-router/dev": "^7.8.2",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20.19.17",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^5.0.2",
    "cross-env": "^7.0.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.6",
    "vite-plugin-pwa": "^1.0.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
